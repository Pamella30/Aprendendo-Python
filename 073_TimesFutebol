times = ('Sao Paulo', 'Santos', 'Palmeiras', 'Corinthias')
print('=' * 30)
print(f'Os 2 primeiros sao {times[0:2]}')
print('=' * 30)
print(f'Os 2 ultimos sao {times[-2:]}')
print('=' * 30)
print(f'Ordem alfabetica: {sorted(times)}')
print('=' * 30)
print(f'O Santos esta na {times.index("Santos")+1} posicao.')